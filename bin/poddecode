use strict;
use warnings;

use Pod::L10N::Model;

{

    my @slurp;

    {
	open my $f, '<', $ARGV[0];
	@slurp = (<$f>);
	close $f;
    }


    my $sl = join '', @slurp;
    my $dl = Pod::L10N::Model::decode($sl);
    
    my $c = 0;
    my $ok = 0;
    
    for (@$dl){
	my ($en, $jp) = @$_;
	print $en . "\n\n";
    }
}
